{
    "name": "chippyash/semantic-version-updater",
    "description": "Utility to update version tags during Jenkins builds",
    "minimum-stability": "stable",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ashley Kitson",
            "email": "ashley@zf4.biz"
        }
    ],
    "require": {
        "zendframework/zend-console": ">=2.6,<3",
        "zendframework/zend-stdlib": ">=2.7.7,<3",
        "zendframework/zend-filter": ">=2.7.2,<3",
        "zendframework/zend-validator": ">=2.9.1,<3",
        "zendframework/zend-i18n": ">=2.7.4,<3",
        "zendframework/zend-log": ">=2.9.2,<3",
        "zendframework/zend-servicemanager": ">=2.7.8,<3",
        "chippyash/validation": ">=1.2,<2"
    },
    "bin": [
        "bin/vupdate"
    ],
    "autoload": {
        "psr-0": {
            "Herrera\\Version": "src"
